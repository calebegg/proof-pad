@top Program { expression* }

expression {
  Keyword |
  Identifier |
  String |
  Symbol |
  Application { "(" expression* ")" }
}

Keyword { @specialize<Identifier, "defun" | "defthm" | "if" | "cond"> }

@tokens {
  Identifier { $[a-zA-Z0-9:&-]+ }

  String { '"' (!["\\] | "\\" _)* '"' }

  LineComment { ";" ![\n]* }

  Symbol { "'" $[a-zA-Z0-9:&-]+ }

  space { $[ \t\n\r]+ }

  "(" ")"
}

@skip { space | LineComment }

@detectDelim
